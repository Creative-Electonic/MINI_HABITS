{
  "private": true,
  "name": "mini-habits",
  "workspaces": {
    "packages": [
      "api",
      "web",
      "packages/*"
    ]
  },
  "devDependencies": {
    "@redwoodjs/core": "^1.4.0",
    "@types/ua-parser-js": "^0.7.36"
  },
  "eslintConfig": {
    "extends": "@redwoodjs/eslint-config",
    "root": true
  },
  "engines": {
    "node": ">=14.19 <=16.x",
    "yarn": ">=1.15"
  },
  "prisma": {
    "seed": "yarn rw exec seed"
  },
  "scripts": {
    "upgrade": "yarn rw upgrade",
    "dev": "yarn rw dev",
    "sync": "yarn rw prisma migrate dev",
    "db": "yarn rw prisma studio",
    "add:web": "yarn workspace web add",
    "build": "yarn rw build"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "dayjs": "^1.11.2",
    "ua-parser-js": "^1.0.2"
  }
}
